  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsc",
    "dev": "npm run clean && tsc-watch --onSuccess \"npm run watch:server\"",
    "watch:server": "nodemon --watch ./src --ext ts --exec ts-node ./src/index.ts",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier src --check",
    "prettier:fix": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts)\""
  },
