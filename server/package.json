    "@types/mongoose-sequence": "^3.0.11",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-async-handler": "^1.2.0",
    "express-validator": "^7.2.0",
    "http-status": "^2.0.0",
    "mongoose": "^8.8.4",
    "mongoose-sequence": "^6.0.1",
